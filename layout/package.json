{
  "name": "industrial-ui-docs-layout",
  "description": "Multilingual documentation for all Industial-ui supported frameworks",
  "scripts": {
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
    "landing:dev": "sapper dev --routes src/landing",
    "landing:build": "cross-env NODE_ENV=production sapper export --legacy --routes src/landing",
    "editor:dev": "sapper dev --routes src/editor",
    "editor:build": "cross-env NODE_ENV=production sapper export --legacy --routes src/editor",
    "layout:dev": "sapper dev --routes src/layout",
    "layout:build": "cross-env NODE_ENV=production sapper export --legacy --routes src/layout",
    "dev": "run-p validate:dev layout:dev",
    "landing-dev": "run-p validate:dev landing:dev",
    "editor-dev": "run-p validate:dev editor:dev"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "sirv": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@editorjs/code": "^2.6.0",
    "@editorjs/editorjs": "^2.19.0",
    "@editorjs/header": "^2.6.1",
    "@editorjs/list": "^1.6.1",
    "@editorjs/simple-image": "^1.4.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/compression": "^1.7.0",
    "@types/polka": "^0.5.1",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.32.1",
    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.29.0",
    "svelte-check": "^1.0.60",
    "svelte-preprocess": "^4.5.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  }
}
